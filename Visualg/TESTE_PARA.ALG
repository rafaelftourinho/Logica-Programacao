Algoritmo "teste_para"

Var

   N, x, soma, i : inteiro

Inicio

   escreva("Digite números que serão digitados: ")
   leia(N)
   
   soma <- 0
   

   para i de 1 ate N faca
        escreva("Digite um número: ")
        leia(x)
        soma <- soma + x
   fimpara
    
   escreval("SOMA = ", soma)

Fimalgoritmo