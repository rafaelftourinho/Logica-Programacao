Algoritmo "Exercicio_fixacao"

Var
   produto1 : caractere
   produto2 : caractere
   preco1 : real
   preco2 : real
   codigo: inteiro
   idade: inteiro
   genero: caractere

Inicio
   produto1 <- "Computador"
   produto2 <- "TV"
   preco1 <- 2100.50
   preco2 <- 1830.00
   idade <- 30
   codigo <- 5291
   genero <- "F"

   escreval(" Produtos: ")
   escreval( " O produto ", produto1, " custa R$", preco1:8:2)
   escreval(" O produto ", produto2, " custa R$", preco2:8:2)
   escreval
   escreval(" Codigo = ",codigo)
   escreval
   escreval(" Dados da pessoa: genero ", genero, " e idade", idade)


Fimalgoritmo